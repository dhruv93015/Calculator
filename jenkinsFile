pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh '''
                python3 Calculator/calculator.py
                '''
            }
        }
        stage('Test') {
            steps {
                sh '''
                python3 Calculator/CalcTest.py 
                '''
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deployed'
            }
        }
    }
}
